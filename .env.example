# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Database URL (PostgreSQL)
DB_URL=postgresql://user:password@host:5432/database?sslmode=require

# Groq API Key (get from https://console.groq.com)
GROQ_API_KEY=your_groq_api_key_here

# OCR.space API Key (optional, default: helloworld for free tier)
# Free tier: 25,000 requests/month (500/day)
# Get custom key from https://ocr.space/ocrapi
OCR_SPACE_API_KEY=helloworld

# Server Port (optional, default: 3000)
PORT=3000
